{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "ALLOWALL" },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self' https: 'unsafe-inline' 'unsafe-eval'; frame-ancestors 'self' https://*.telegram.org;"
        }
      ]
    }
  ]
}
